---
import PageLayout from '../../layouts/EnPageLayout.astro'
---

<PageLayout>
  <section
    style="background-image: url(/trystavisim/images/full-width-images/convert_bg_lowq.jpg)"
    class="page-section padding-bot-null margin-bot dark-bg image-bg"
  >
    <div class="bg-overlay-1"></div>
    <div class="container bg-content">
      <div class="row">
        <div class="col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2">
          <div class="section-title">Tour Dates</div>
          <h1 class="section-heading">Tour Archive</h1>
        </div>
      </div>
    </div>
  </section>

  <section>
    <div class="container">
      <div class="section-text margin-bot-null text-center">To be announced</div>
      <br />
      <div class="row text-center">
        <p>
          <a href="/trystavisim/en/arhiv-koncertiv" class="btn btn-spacia btn-large">Tour Archive</a
          >
        </p>
      </div>
    </div>
  </section>
</PageLayout>
